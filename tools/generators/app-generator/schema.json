{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "app-generator",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Application name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "请输入 Application name："
    },
    "port": {
      "type": "string",
      "description": "Application port",
      "default": "5000",
      "x-prompt": "请输入 Application port："
    }
  },
  "required": ["name"]
}
